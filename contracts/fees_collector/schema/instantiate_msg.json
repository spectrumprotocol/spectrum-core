{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "InstantiateMsg",
  "description": "This structure stores general parameters for the contract.",
  "type": "object",
  "required": [
    "operator",
    "owner",
    "router",
    "stablecoin",
    "target_list"
  ],
  "properties": {
    "operator": {
      "description": "Address that's allowed to update bridge assets",
      "type": "string"
    },
    "owner": {
      "description": "Address that's allowed to update config",
      "type": "string"
    },
    "router": {
      "description": "The factory contract address",
      "type": "string"
    },
    "stablecoin": {
      "description": "The stablecoin asset info",
      "allOf": [
        {
          "$ref": "#/definitions/Denom"
        }
      ]
    },
    "target_list": {
      "description": "The beneficiary addresses to received fees in stablecoin",
      "type": "array",
      "items": {
        "type": "array",
        "items": [
          {
            "type": "string"
          },
          {
            "type": "integer",
            "format": "uint64",
            "minimum": 0.0
          }
        ],
        "maxItems": 2,
        "minItems": 2
      }
    }
  },
  "definitions": {
    "Denom": {
      "type": "string"
    }
  }
}
